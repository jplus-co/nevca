<ul class="press-archive-list">
  {% for post in props %}
    {% include "list-card.twig" with {
      "link": post.get_field('link')|relative,
      "target": "_blank",
      "date_circle": post.publish_date,
      "subheading": post.source,
      "heading": post.title,
      "action": 'Read article',
      "list_card_classes": 'mb-7'
    } %}
  {% endfor %}
</ul>
